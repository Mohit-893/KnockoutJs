@page
@model readDataFromCSV.Pages.Employee.IndexModel
@{
}
<!-- CSS only -->
@*<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">*@
<br />
<h2>Employee Data</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>DOB</th>
            <th>DOJ</th>
            <th>City</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ValidEmployees)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.DOB</td>
                <td>@item.DOJ</td>
                <td>@item.City</td>
                <td>@item.State</td>
            </tr>
        }
    </tbody>
</table>